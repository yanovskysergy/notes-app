{
  "singleQuote": false,
  "trailingComma": "all",
  "useTabs": false,
  "tabWidth": 2,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react(.*)", 
    "<THIRD_PARTY_MODULES>",
    "@/(.*)",
    "@shared/*",
    "^[./]",
    "scss$"
  ],
  "importOrderSeparation": false, 
  "importOrderSortSpecifiers": true 
}